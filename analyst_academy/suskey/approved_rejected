select COUNT(DISTINCT case_id)
from REPORTS.META_EDD_INVESTIGATIONS_CASE
where EDD_RULE ilike '%SuspiciousKeywordRule%'
AND CASE_CREATION_TIME BETWEEN '2022-09-13' AND '2023-08-10'
AND CASE_STATE = 'APPROVED'
AND FIRST_CUSTOMER_CONTACT_TIMESTAMP IS NULL;


select COUNT(DISTINCT CASE_ID)
from REPORTS.META_EDD_INVESTIGATIONS_CASE
where EDD_RULE ilike '%SuspiciousKeywordRule%'
AND CASE_CREATION_TIME BETWEEN '2022-09-13' AND '2023-08-10'
AND CASE_STATE = 'REJECTED'
AND FIRST_CUSTOMER_CONTACT_TIMESTAMP IS NULL;
